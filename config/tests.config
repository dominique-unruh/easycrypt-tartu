[default]
bin  = ./ec.native

[test-unit]
okdirs  = tests/*/success
kodirs  = tests/*/fail
exclude = tests/pbhl/success tests/pbhl/failure

[test-prelude]
args   = -no-evict Alt-Ergo -p Eprover -p Alt-Ergo -p Z3 -boot
okdirs = theories/prelude

[test-core]
args   = -no-evict Alt-Ergo -p Eprover -p Alt-Ergo -p Z3
okdirs = theories/core

[test-theories]
args      = -no-evict Alt-Ergo -p Eprover -p Alt-Ergo -p Z3
okdirs    = theories
cpufactor = theories/FSet.ec:2 theories/Matrix.ec:2

[test-newth]
args   = -no-evict Alt-Ergo -p Eprover -p Alt-Ergo -p Z3
okdirs = theories/newth

[test-realized]
args   = -no-evict Alt-Ergo -p Eprover -p Alt-Ergo -p Z3
okdirs = theories/realizations

[test-examples]
okdirs = examples
